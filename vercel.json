{
  "builds": [
    {
      "src": "my_site/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "https://spacetech-bucket-2.s3.amazonaws.com/static/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "my_site/wsgi.py"
    }
  ]
}
